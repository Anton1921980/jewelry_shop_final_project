{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"   
           
        }
        
    ],
    "functions": {
        "pages/api/**": {
          "excludeFiles": "{public,.next,pages,images,_next,gen}/**"
        }
      }
  }